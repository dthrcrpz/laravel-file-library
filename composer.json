{
    "name": "dthrcrpz/laravel-file-library",
    "description": "A simple package to upload files and attach them to your Models",
    "keywords": ["library", "laravel", "files"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Deither Corpuz",
            "email": "dthrcrpz@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^10.0",
        "intervention/image": "^3.0",
        "laravel/helpers": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Dthrcrpz\\FileLibrary\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dthrcrpz\\FileLibrary\\Providers\\FileServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
